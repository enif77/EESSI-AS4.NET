<?xml version="1.0" encoding="utf-8"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns="eu:edelivery:as4:pmode">
  <Id>integration-encrypted-issuerserial-signed</Id>
  <AllowOverride>false</AllowOverride>
  <PushConfiguration>
    <Protocol>
      <Url>http://localhost:9090/msh</Url>
    </Protocol>
  </PushConfiguration>
  <ReceiptHandling>
    <NotifyMessageProducer>true</NotifyMessageProducer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\receipts\" />
      </Parameters>
    </NotifyMethod>
  </ReceiptHandling>
  <Security>
    <Signing>
      <IsEnabled>true</IsEnabled>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>AccessPointA</CertificateFindValue>
      </CertificateFindCriteria>
      <KeyReferenceMethod>IssuerSerial</KeyReferenceMethod>
      <Algorithm>http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</Algorithm>
      <HashFunction>http://www.w3.org/2001/04/xmlenc#sha256</HashFunction>
    </Signing>
    <Encryption>
      <IsEnabled>true</IsEnabled>
      <Algorithm>http://www.w3.org/2009/xmlenc11#aes128-gcm</Algorithm>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>AccessPointB</CertificateFindValue>
      </CertificateFindCriteria>
    </Encryption>
  </Security>
  <MessagePackaging>
    <UseAS4Compression>true</UseAS4Compression>
    <IsMultiHop>false</IsMultiHop>
    <IncludePModeId>true</IncludePModeId>
    <PartyInfo>
      <FromParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example</Id>
          </PartyId>
        </PartyIds>
        <Role>Sender</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>org:holodeckb2b:example:company:B</Id>
          </PartyId>
        </PartyIds>
        <Role>Receiver</Role>
      </ToParty>
    </PartyInfo>
  </MessagePackaging>
</PMode>